{
  "name": "agrowcrop-backend",
  "version": "1.0.0",
  "description": "AgrowCrop Backend - AI-powered smart farming platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "frontend": "cd frontend && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run frontend\"",
    "start:all": "concurrently \"npm start\" \"npm run frontend\"",
    "backend:java": "cd java-service && mvnw.cmd spring-boot:run -DskipTests",
    "dev:java": "concurrently \"npm run backend:java\" \"npm run frontend\""
  },
  "keywords": ["farming", "agriculture", "crop-recommendation"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "axios": "^1.5.0",
    "csv-parser": "^3.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}

